ay: flex;\n    margin: 0;\n    padding: 0;\n  }\n\n  &__item {\n    display: inline-block;\n    position: relative;\n    margin-right: 20px;\n\n    &:last-child {\n      margin: 0;\n    }\n  }\n\n  &__link {\n    display: inline-block;\n    line-height: 60px;\n    color: #000;\n    font-size: 12px;\n    font-weight: 500;\n    text-transform: uppercase;\n    text-decoration: none;\n    text-align: center;\n\n    &--hover:hover {\n      color: #00acdc;\n    }\n  }\n}\n\n.is-active {\n  color: #00acdc;\n}\n\n.is-active::after {\n  content: \" \";\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  height: 4px;\n  width: 100%;\n  border-radius: 8px;\n  background: #00acdc;\n}\n","styles/blocks/catalog.scss":".catalog {\n  display: grid;\n  grid-template-columns: repeat(4, 200px);\n  justify-content: center;\n  gap: 49px 48px;\n  padding-top: 50px;\n  padding-bottom: 77px;\n}\n","styles/blocks/card.scss":".card {\n  display: flex;\n  flex-direction: column;\n  width: 200px;\n  height: 403px;\n  padding: 32px 16px 16px;\n  background-color: #fff;\n  border: 1px solid #f3f3f3;\n  border-radius: 5px;\n\n  &__img {\n    width: 160px;\n    height: 134px;\n    margin: 0 auto 40px;\n  }\n\n  &__product-name {\n    font-size: 12px;\n    line-height: 18px;\n    margin-bottom: 2px;\n    font-weight: 500;\n  }\n\n  &__product-number {\n    font-size: 10px;\n    line-height: 14px;\n    margin-bottom: 16px;\n    color: #616070;\n  }\n\n  &__feedback {\n    display: flex;\n    justify-content: space-between;\n  }\n\n  &__price {\n    display: flex;\n    justify-content: space-between;\n    width: 100%;\n  }\n\n  &__price-text {\n    font-size: 12px;\n    line-height: 18px;\n    color: #616070;\n  }\n\n  &__price-number {\n    font-size: 16px;\n    font-weight: bold;\n    margin-bottom: 15px;\n    line-height: 18px;\n  }\n}\n\n.feedback__rate {\n  display: flex;\n}\n\n.feedback__stars {\n  margin-right: 4px;\n  background-image: url(../images/star.svg);\n  background-repeat: no-repeat;\n  background-position: center;\n  height: 16px;\n  width: 16px;\n}\n\n.feedback__rate--1 .feedback__stars:nth-child(-n + 1),\n.feedback__rate--2 .feedback__stars:nth-child(-n + 2),\n.feedback__rate--3 .feedback__stars:nth-child(-n + 3),\n.feedback__rate--4 .feedback__stars:nth-child(-n + 4),\n.feedback__rate--5 .feedback__stars:nth-child(-n + 5) {\n  background-image: url(../images/star-active.svg);\n}\n\n.feedback__text {\n  font-size: 10px;\n  line-height: 14px;\n  margin-bottom: 24px;\n}\n\n.buy-link {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  width: 168px;\n  height: 40px;\n  font-size: 14px;\n  font-weight: bold;\n  line-height: 16px;\n  text-transform: uppercase;\n  text-decoration: none;\n  background-color: #00acdc;\n  border-radius: 5px;\n  color: #fff;\n\n  &:hover {\n    background-color: #fff;\n    color: #00acdc;\n    border: 1px solid #00acdc;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"508a781420af5e39185c72c9a4201688","cacheData":{"env":{}}}