te;\n  bottom: 0;\n  left: 0;\n  height: 4px;\n  width: 100%;\n  border-radius: 8px;\n  background: #00acdc;\n}\n","styles/blocks/catalog.scss":".catalog {\n  display: flex;\n  flex-wrap: wrap;\n  padding: 50px 128px 28px;\n  justify-content: space-around;\n  align-content: space-between;\n}\n\n.catalog__card--margin-bot {\n  display: flex;\n  margin-bottom: 50px;\n}\n\n.catalog__card:not(:last-child) {\n  margin-right: 48px;\n}\n","styles/blocks/card.scss":".card {\n  display: flex;\n  flex-direction: column;\n  width: 200px;\n  height: 403px;\n  padding: 32px 16px 16px;\n  background-color: #fff;\n  border: 1px solid #f3f3f3;\n  border-radius: 5px;\n\n  &__img {\n    width: 160px;\n    height: 134px;\n    margin: 0 auto 40px;\n  }\n\n  &__product-name {\n    font-size: 12px;\n    line-height: 18px;\n    margin-bottom: 2px;\n    font-weight: 500;\n  }\n\n  &__product-number {\n    font-size: 10px;\n    line-height: 14px;\n    margin-bottom: 16px;\n    color: #616070;\n  }\n\n  &__feedback {\n    display: flex;\n    justify-content: space-between;\n  }\n\n  &__price {\n    display: flex;\n    justify-content: space-between;\n    width: 100%;\n  }\n\n  &__price-text {\n    font-size: 12px;\n    line-height: 18px;\n    color: #616070;\n  }\n\n  &__price-number {\n    font-size: 16px;\n    font-weight: bold;\n    margin-bottom: 15px;\n    line-height: 18px;\n  }\n}\n\n.feedback__rate {\n  display: flex;\n}\n\n.feedback__stars {\n  margin-right: 4px;\n  background-image: url(../images/star.svg);\n  background-repeat: no-repeat;\n  background-position: center;\n  height: 16px;\n  width: 16px;\n}\n\n.feedback__rate--1 .feedback__stars:nth-child(-n + 1),\n.feedback__rate--2 .feedback__stars:nth-child(-n + 2),\n.feedback__rate--3 .feedback__stars:nth-child(-n + 3),\n.feedback__rate--4 .feedback__stars:nth-child(-n + 4),\n.feedback__rate--5 .feedback__stars:nth-child(-n + 5) {\n  background-image: url(../images/star-active.svg);\n}\n\n.feedback__text {\n  font-size: 10px;\n 